function setup(name)

    parseCalbody('pa1-debug-a-calbody.txt');

    function parseCalbody(filename)
        M = csvread(filename, 1, 0);
        info = fileread(filename);
        info = strsplit(info(1), ',');
        a = (info(1));
        dN = str2num(info(1));
        aN = str2num(info(2));
        cN = str2num(info(3));
        d = M(1:dN, :,:);
        a = M(dN:(aN+dN), :,:);
        c = M((aN+dN):(aN+dN+cN), :,:);
        
    end

end